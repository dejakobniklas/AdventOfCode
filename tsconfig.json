{
    "$schema": "https://json.schemastore.org/tsconfig",
    "compilerOptions": {
        "module": "commonjs",
        "target": "esnext",
        "lib": [ "esnext" ],
        "esModuleInterop": true,
        "sourceMap": true,
        "rootDir": ".",
        "strict": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "noImplicitAny": true,
        "noImplicitOverride": true,
        "typeRoots": [
            "./node_modules/@types"
        ]
    },
    "exclude": [
        "node_modules",
        "previous"
    ]
}